{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "allowJs": true,
    "importHelpers": true,
    "jsx": "react",
    "alwaysStrict": true,
    "sourceMap": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitAny": false,
    "noImplicitThis": false,
    "strictNullChecks": false,
    "esModuleInterop": true,
    "outDir": "build"
  },
  "include": [
    "src/**/*",
    "test-utils/**/*"
, "test-utils/setup/create-job.ts", "util/mocks", "test-utils/setup/setup.ts", "test-utils/probot.ts", "test-utils/setup/tracking/index.ts", "test-utils/setup/matchers/nock.ts"  ]
}
