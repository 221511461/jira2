{
	"compilerOptions": {
		"target": "ES2020",
		"module": "commonjs",
		"moduleResolution": "node",
		"allowSyntheticDefaultImports": true,
		"allowJs": true,
		"importHelpers": true,
		"jsx": "react",
		"alwaysStrict": true,
		"strictNullChecks": true,
		"strictBindCallApply": true,
		"strictFunctionTypes": true,
		"sourceMap": true,
		"forceConsistentCasingInFileNames": true,
		"noFallthroughCasesInSwitch": true,
		"noImplicitReturns": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"noImplicitAny": false,
		"noImplicitThis": false,
		"esModuleInterop": true,
		"resolveJsonModule": true,
		"outDir": "build",
		"baseUrl": ".",
		"paths": {
			"routes": [
				"src/routes"
			],
			"middleware": [
				"src/middleware"
			],
			"utils": [
				"src/util"
			],
			"config": [
				"src/config"
			],
			"interfaces": [
				"src/interfaces"
			],
			"services": [
				"src/services"
			]
		}
	},
	"include": [
		"src/**/*",
		"test/**/*"
	]
}
