# The ID of your GitHub App
APP_ID=
APP_URL=
WEBHOOK_SECRET=development
GITHUB_CLIENT_ID=
GITHUB_CLIENT_SECRET=
NODE_ENV=production
TUNNEL_PORT=8080
TUNNEL_SUBDOMAIN=<yourname>-jira

# Path to the file containing the private key to the GitHub app (*.pem). Use either this or PRIVATE_KEY.
PRIVATE_KEY_PATH=

# String containing the private key to the GitHub app. Use either this or PRIVATE_KEY_PATH.
PRIVATE_KEY=

ATLASSIAN_URL=

# Use `trace` to get verbose logging or `info` to show less
LOG_LEVEL=debug

WEBHOOK_PROXY_URL=

# The Postgres URL used to connect to the database and secret for encrypting data
DATABASE_URL=
STORAGE_SECRET=

# Unique identifier for this instance, used in the Atlassian Connect manifest to
# differentiate this instance from other deployments (staging, dev instances, etc).
# INSTANCE_NAME=<you-username>
