{
  "development": {
    "dialect": "postgres",
    "use_env_variable": "DATABASE_URL"
  },
  "test": {
    "dialect": "postgres",
    "use_env_variable": "DATABASE_URL"
  },
  "production": {
    "dialect": "postgres",
    "use_env_variable": "DATABASE_URL",
    "disable_sql_logging": true,
    "ssl": true,
    "dialectOptions": {
      "ssl": {
        "require": true,
        "rejectUnauthorized": false
      }
    },
    "pool": {
      "max": 15
    }
  }
}
