{
  "development": {
    "dialect": "postgres",
    "use_env_variable": "DATABASE_URL"
  },
  "test": {
    "dialect": "postgres",
    "use_env_variable": "DATABASE_URL"
  },
  "production": {
    "dialect": "postgres",
    "use_env_variable": "PG_DATABASE_URL",
    "disable_sql_logging": true,
    "ssl": false,
    "dialectOptions": {
      "ssl": false
    },
    "pool": {
      "max": 15
    }
  }
}
