{
	"name": "@atlassian/github-for-jira",
	"version": "0.0.0",
	"description": "GitHub integration for Atlassian Jira",
	"repository": "https://github.com/atlassian/github-for-jira.git",
	"engines": {
		"node": ">= 18.14.0",
		"yarn": "^1.22.0"
	},
	"main": "src/main",
	"scripts": {
		"prestart": "run-p setup clean db:dev",
		"postinstall": "cd spa && yarn",
		"clean": "rimraf coverage build tmp src/**/*.js src/**/*.js.map test/**/*.js test/**/*.js.map",
		"init": "run-p db",
		"start": "run-p spa:dev start:main start:worker",
		"start:no-spa": "run-p start:main start:worker",
		"start:main": "tsnd -r tsconfig-paths/register --watch=.env*,db/config.json --inspect=0.0.0.0:9229 --respawn --transpile-only -- src/main.ts",
		"start:worker": "tsnd -r tsconfig-paths/register --watch=.env*,db/config.json --inspect=0.0.0.0:9230 --respawn --transpile-only -- src/worker.ts",
		"start:production": "run-p start:main:production start:worker:production",
		"start:main:production": "node -r tsconfig-paths/register src/main",
		"start:worker:production": "node -r tsconfig-paths/register src/worker",
		"setup": "dotenv -- ts-node -r tsconfig-paths/register prestart.ts",
		"build": "tsc -p ./tsconfig.json && yarn spa:build",
		"build:release": "tsc -p ./tsconfig.release.json && yarn spa:build",
		"build:watch": "tsc -w -p ./tsconfig.json && yarn spa:build",
		"lint": "run-p lint:*",
		"lint:ts": "eslint . --ext .ts,.tsx",
		"lint:yaml": "yamllint github-for-jira.sd.yml",
		"pretest": "NODE_ENV=test run-p clean setup db:test",
		"test": "yarn spa:test && jest --runInBand --forceExit --coverage",
		"test:watch": "jest --runInBand --watchAll --coverage=false",
		"db": "run-s db:create db:migrate",
		"db:create": "run-p db:create:dev db:create:test",
		"db:create:dev": "dotenv -c development -- sequelize db:create || exit 0",
		"db:create:test": "dotenv -c test -- sequelize db:create --env test || exit 0",
		"db:create:prod": "sequelize db:create --env production-migrate || exit 0",
		"db:migrate": "run-p db:migrate:dev db:migrate:test",
		"db:migrate:dev": "dotenv -c development -- sequelize db:migrate",
		"db:migrate:test": "dotenv -c test -- sequelize db:migrate --env test",
		"db:migrate:prod": "sequelize db:migrate --env production-migrate",
		"db:migrate:create": "yarn run sequelize migration:create --name",
		"db:dev": "run-s db:create:dev db:migrate:dev",
		"db:test": "run-s db:create:test db:migrate:test",
		"prepare": "husky install",
		"precommit": "run-p lint",
		"test:e2e": "yarn test:e2e:install && yarn test:e2e:start",
		"test:e2e:headed": "yarn test:e2e --headed",
		"test:e2e:install": "playwright install",
		"test:e2e:start": "NODE_ENV=e2e playwright test",
		"test:e2e:start:headed": "yarn test:e2e:start --headed",
		"test:e2e:debug": "PWDEBUG=1 yarn test:e2e:headed",
		"test:e2e:watch": "nodemon --watch test/e2e -e ts --exec 'yarn test:e2e'",
		"test:e2e:watch:headed": "nodemon --watch test/e2e -e ts --exec 'yarn test:e2e:headed'",
		"spa:dev": "cd spa && yarn dev",
		"spa:build": "etc/spa-build/spa-build.sh",
		"spa:test": "cd spa && yarn test"
	},
	"dependencies": {
		"@atlaskit/css-reset": "^6.5.4",
		"@atlaskit/reduced-ui-pack": "^18.0.5",
		"@atlassian/aui": "^9.7.1",
		"@octokit/auth-app": "^6.0.0",
		"@octokit/graphql-schema": "^14.25.0",
		"@sentry/node": "^7.61.0",
		"atlassian-jwt": "^2.0.2",
		"aws-sdk": "^2.1428.0",
		"axios": "^1.4.0",
		"body-parser": "^1.20.2",
		"bottleneck": "^2.19.5",
		"bunyan": "^1.8.15",
		"bunyan-format": "^0.2.1",
		"cookie-parser": "^1.4.6",
		"cookie-session": "^2.0.0",
		"csurf": "^1.11.0",
		"date-fns": "^2.30.0",
		"dotenv": "^16.3.1",
		"dotenv-expand": "^10.0.0",
		"express": "^4.18.2",
		"express-async-handler": "^1.2.0",
		"express-rate-limit": "^6.8.1",
		"express-sslify": "^1.2.0",
		"express-validator": "^7.0.1",
		"hbs": "^4.2.0",
		"helmet": "^7.0.0",
		"hot-shots": "^10.0.0",
		"http-proxy": "^1.18.1",
		"https-proxy-agent": "7.0.1",
		"ioredis": "^5.3.2",
		"is-base64": "^1.1.0",
		"launchdarkly-node-server-sdk": "^7.0.2",
		"lodash": "^4.17.21",
		"matchstick": "^1.2.1",
		"minimatch": "^9.0.3",
		"moment": "^2.29.4",
		"moo": "^0.5.2",
		"node_extra_ca_certs_mozilla_bundle": "^1.0.5",
		"optional-require": "^1.1.8",
		"pg": "^8.11.2",
		"primer": "^10.6.0",
		"rate-limit-redis": "^3.0.2",
		"safe-json-stringify": "^1.2.0",
		"sanitize-html": "^2.11.0",
		"sequelize": "^6.32.1",
		"sequelize-cli": "^6.6.1",
		"sequelize-encrypted": "^1.0.0",
		"speakeasy": "^2.0.0",
		"sqs-consumer": "^7.2.2",
		"throng": "^5.0.0",
		"ts-retry-promise": "^0.7.0",
		"tsconfig-paths": "^4.2.0",
		"tslib": "~2.6.1",
		"typescript": "~5.1.6",
		"uuid": "^9.0.0",
		"xml2json-light": "^1.0.6",
		"yaml": "^2.3.1"
	},
	"devDependencies": {
		"@octokit/rest": "20.0.1",
		"@octokit/webhooks-types": "^7.1.0",
		"@playwright/test": "^1.36.2",
		"@types/bunyan": "^1.8.8",
		"@types/bunyan-format": "^0.2.5",
		"@types/cache-manager": "^4.0.2",
		"@types/chai": "^4.3.5",
		"@types/cookie-session": "^2.0.44",
		"@types/csurf": "^1.11.2",
		"@types/express": "^4.17.17",
		"@types/express-rate-limit": "^5.1.3",
		"@types/hbs": "^4.0.1",
		"@types/jest": "~29.5.3",
		"@types/jest-when": "^3.5.2",
		"@types/lodash": "^4.14.196",
		"@types/moo": "^0.5.5",
		"@types/newrelic": "^9.14.0",
		"@types/node": "~20.4.6",
		"@types/rate-limit-redis": "^1.7.4",
		"@types/supertest": "^2.0.12",
		"@types/uuid": "^9.0.2",
		"@typescript-eslint/eslint-plugin": "~6.2.1",
		"@typescript-eslint/parser": "~6.2.1",
		"chai": "^4.3.7",
		"dotenv-cli": "^7.2.1",
		"eslint": "~8.46.0",
		"eslint-config-prettier": "~8.9.0",
		"eslint-import-resolver-typescript": "^3.5.5",
		"eslint-plugin-import": "^2.28.0",
		"eslint-plugin-jest": "~27.2.3",
		"eslint-plugin-jsdoc": "^46.4.5",
		"eslint-plugin-no-only-tests": "^3.1.0",
		"eslint-plugin-node": "^11.1.0",
		"eslint-plugin-promise": "^6.1.1",
		"husky": "^8.0.3",
		"jest": "^29.6.2",
		"jest-diff": "^29.6.2",
		"jest-when": "^3.5.2",
		"keygrip": "^1.1.0",
		"nock": "^13.3.2",
		"nodemon": "^3.0.1",
		"npm-run-all": "^4.1.5",
		"playwright": "^1.36.2",
		"prettier": "~3.0.0",
		"rimraf": "~5.0.1",
		"smee-client": "^1.2.3",
		"sqlite3": "^5.1.6",
		"supertest": "^6.3.3",
		"ts-jest": "~29.1.1",
		"ts-node": "^10.9.1",
		"ts-node-dev": "^2.0.0",
		"yaml-lint": "^1.7.0"
	},
	"optionalDependencies": {
		"@atlassian/sqs-queue-dlq-service": "^2.1.1",
		"@atlassiansox/analytics-node-client": "^3.3.0"
	},
	"volta": {
		"node": "14.21.1",
		"yarn": "1.22.18"
	}
}
