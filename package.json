{
  "name": "github-for-jira",
  "version": "0.0.0",
  "description": "Github integration for Atlassian Jira",
  "repository": "https://github.com/integrations/jira.git",
  "engines": {
    "node": ">= 14.16 <15",
    "npm" : "^6.12.0"
  },
  "main": "src/main",
  "scripts": {
    "start": "run-p start:main start:worker",
    "start:main": "ts-node .",
    "start:worker": "ts-node src/worker",
    "start:production": "node .",
    "clean": "rimraf coverage build tmp",
    "build": "tsc -p ./tsconfig.json",
    "build:release": "tsc -p ./tsconfig.release.json",
    "build:watch": "tsc -w -p ./tsconfig.json",
    "lint": "eslint . --ext .ts,.tsx",
    "test": "jest --runInBand --detectOpenHandles --coverage",
    "test:watch": "jest --runInBand --detectOpenHandles --watchAll",
    "prepare": "husky install"
  },
