#!/usr/bin/env node
require('dotenv').config()
const throng = require('throng')

require('newrelic')
require('../lib/config/sentry').initializeSentry()

const worker = require('../lib/worker')
const throngWorkers = process.env.WEB_CONCURRENCY || 1

throng({
  workers: throngWorkers,
  lifetime: Infinity
}, worker.start)
